{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "SMRT Server Health Schema",
    "type": "object",
    "properties": {
        "id": {
            "type": "string"
        },
        "message": {
            "type": "string"
        },
        "uptime": {
            "type": "integer",
            "description": "Uptime in seconds of the Service.",
            "title": "Service Uptime (sec)"
        }
    },

    "required": ["id", "message", "uptime"],
    "additionalProperties": false
}