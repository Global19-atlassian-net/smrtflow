{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "pacbio.schemas.components",
    "version": "0.1.0",
    "title": "SMRT Server Component Schema",
    "description": "SMRT Server components are Services, Tools or SMRT Apps. Each component has a list of component dependencies.",
    "type": "object",
    "properties": {
        "id": {
            "type": "string"
        },
        "version": {
            "type": "string"
        }
    },

    "required": ["id", "version"],
    "additionalProperties": false
}